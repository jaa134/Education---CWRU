public class Test4 {
  public static void reverse(int[] original) {
    int[] temporary = new int[original.length];
    for(int i = 0; i < original.length; i++) {
      temporary[i] = original[original.length - i - 1];
    }
    for(int i = 0; i < original.length; i++) {
      original[i] = temporary[i];
    }
  }
}
  
  